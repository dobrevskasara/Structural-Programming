//Напиши програма коjа ќе прави конверзија на Целзиусови во Фаренхајтови
//степени. Целзиусовите степени се внесуваат од тастатура, а резултатот е потребно
//да се отпечати на екран.Формулата за претворањето е f = 1.8C + 32.

#include <iostream>
using namespace std;
int main(){
    int celsius;
    cout<<"Vnesi Celzusovi stepeni: "<<endl;
    cin>>celsius;
    double farenhait;
    farenhait=1.8*(double)celsius+32;
    cout<<"Stepenite "<<celsius<<" vo Farenhajtovi iznesuvaat: "<<farenhait;
    return 0;
}
